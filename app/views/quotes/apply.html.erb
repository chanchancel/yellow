    <div id="blog">
      <%= form_for :quote, url: {action: 'create'}, remote: true do |f| %>
        <div class='container'>
          <div class='row'>
            <div class='' style='margin: 0 0 10px 0;'><b><em>Client Details</em></b></div>
            <div class='form-group'>
              <div class='input-group col-md-9'>
                <div class='input-group-addon'>Name</div>
                <%= f.text_field :name, {:class=>'form-control'} %>
              </div>
            </div>
            <div class='form-group form-inline'>
              <div class='input-group col-md-5'>
                <div class='input-group-addon'>Email</div>
                <%= f.text_field :email, {:class=>'form-control'} %>
              </div>
              <div class='input-group col-md-4'>
                <div class='input-group-addon'>Phone</div>
                <%= f.text_field :phone, {:class=>'form-control'} %>
              </div>
            </div>
            <div class='' style='margin: 0 0 10px 0;'><b><em>Cargo Details</em></b></div>
            <div class='form-inline'>
              <div class='input-group'>
                <span class='input-group-addon'>Cargo Type</span>
                <%= f.select :cargo_type, ['Container', 'Less Container Load (LCL)'], {}, :class=> 'form-control selectpicker' %>
              </div>
              <div class='input-group'>
                <span class='input-group-addon'>Container Type</span>
                <%= f.select :container_type, ['20FT', '40FT'], {}, :class=> 'form-control selectpicker', 'data-title'=>'20FT' %>
              </div>
              <div class='input-group'>
                <span class='input-group-addon'>Cargo Weight</span>
                <%= f.text_field :cargo_weight, {:class=>'form-control'} %>
                <span class='input-group-addon'>Tonne</span>
              </div>
            </div>
            <div class='form-group'>
              <div class='control-label'>Cargo Details</div>
              <div class='input-group col-md-9'>
                <%= f.text_area :cargo_details,  {:class=>'form-control', :row=>'4'} %>
              </div>
            </div>
            <div class='form-group'>
              <div class='control-label'>Special Instruction</div>
              <div class='input-group col-md-9'>
                <%= f.text_area :instruction,  {:class=>'form-control', :row=>'4'} %>
              </div>
            </div>
            <div class='' style='margin: 0 0 10px 0;'><b><em>Consignee Details</em></b></div>
            <div class='form-group'>
              <div class='input-group col-md-9'>
                <div class='input-group-addon'>Importer Name</div>
                <%= f.text_field :importer, {:class=>'form-control'} %>
              </div>
            </div>
            <div class='form-group form-inline'>
              <div class='input-group col-md-5'>
                <div class='input-group-addon'>Email</div>
                <%= f.text_field :importer_email, {:class=>'form-control'} %>
              </div>
              <div class='input-group col-md-4'>
                <div class='input-group-addon'>Phone</div>
                <%= f.text_field :importer_phone, {:class=>'form-control'} %>
              </div>
            </div>
            <div class='form-group'>
              <div class='input-group col-md-5'>
                <div class='input-group-addon'>Cargo Origin</div>
                <%= f.text_field :origin, {:class=>'form-control'} %>
              </div>
            </div>
            <%= f.submit 'Request Quote', {:class=>'btn btn-primary'} %>
          </div>
        </div>
      </div>
    <% end %>
